  "main": "gulpfile.js",
  "scripts": {
    "webpack": "webpack",
    "webpack-watch": "webpack --watch -d",
    "build": "cross-env BACKEND_URL=https\\://beeline-server-dev.herokuapp.com BUILD_PREFIX=www webpack",
    "watch": "npm run start & npm run webpack-watch",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "start": "http-server -c-1 www",
    "heroku-postbuild": "cross-env NODE_ENV=production BACKEND_URL=https\\://beeline-server-dev.herokuapp.com BUILD_PREFIX=www webpack",
    "live-build": "cross-env NODE_ENV=production BACKEND_URL=https\\://beeline-server-admin.herokuapp.com BUILD_PREFIX=build webpack",
    "live-cp": "shx mkdir -p build && shx cp -r 'www/*' build",
    "live-cname": "shx echo admin.beeline.sg > build/CNAME",
    "live-gh-pages": "gh-pages -d build",
    "deploy": "npm run live-cp && npm run live-build && npm run live-cname",
    "deploy!": "npm run deploy && npm run live-gh-pages"
  },
